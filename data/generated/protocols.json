          "keyEvents": [
            {
              "name": "IncreasePosition",
              "description": "Emitted when a leveraged position is opened or increased"
            },
            {
              "name": "DecreasePosition",
              "description": "Emitted when a leveraged position is closed or decreased"
            },
            {
              "name": "LiquidatePosition",
              "description": "Emitted when a position is liquidated"
            },
            {
              "name": "ClosePosition",
              "description": "Emitted when a position is fully closed"
            }
          ],
          "useCases": [
            "Track perpetual position opens/closes",
            "Monitor liquidation events",
