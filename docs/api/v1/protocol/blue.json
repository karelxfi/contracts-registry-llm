  "contracts": {
    "morpho": {
      "name": "Morpho Market V1 (Morpho Blue)",
      "type": "core",
      "description": "Main lending protocol contract. Enables supply/withdraw, borrow/repay, collateral management, liquidations, and zero-fee flash loans.",
      "proxy": false,
      "keyEvents": [
        {
          "name": "Supply",
          "signature": "Supply(bytes32 indexed id, address indexed caller, address indexed onBehalf, uint256 assets, uint256 shares)",
          "description": "Emitted when assets are supplied to a market"
        },
        {
          "name": "Withdraw",
          "signature": "Withdraw(bytes32 indexed id, address caller, address indexed onBehalf, address indexed receiver, uint256 assets, uint256 shares)",
          "description": "Emitted when assets are withdrawn from a market"
        },
        {
          "name": "Borrow",
          "signature": "Borrow(bytes32 indexed id, address caller, address indexed onBehalf, address indexed receiver, uint256 assets, uint256 shares)",
          "description": "Emitted when assets are borrowed from a market"
        },
        {
          "name": "Repay",
          "signature": "Repay(bytes32 indexed id, address indexed caller, address indexed onBehalf, uint256 assets, uint256 shares)",
          "description": "Emitted when borrowed assets are repaid"
        },
        {
          "name": "SupplyCollateral",
          "description": "Emitted when collateral is supplied"
        },
        {
          "name": "WithdrawCollateral",
          "description": "Emitted when collateral is withdrawn"
        },
        {
          "name": "Liquidate",
          "description": "Emitted when a position is liquidated"
        }
      ],
      "keyFunctions": [
        {
          "name": "supply",
          "mutability": "nonpayable"
        },
        {
          "name": "withdraw",
          "mutability": "nonpayable"
        },
        {
          "name": "borrow",
          "mutability": "nonpayable"
        },
        {
          "name": "repay",
          "mutability": "nonpayable"
        }
      ],
      "useCases": [
        "Track user deposits and withdrawals by market",
        "Monitor borrow/repay activity with accrued interest",
        "Index liquidation events with collateral seized",
        "Analyze market creation and parameter choices",
        "Track market-level utilization rates"
      ],
      "github": "https://github.com/morpho-org/morpho-blue"
    },
    "adaptiveCurveIrm": {
      "name": "Adaptive Curve Interest Rate Model",
      "type": "peripheral",
      "description": "Morpho's flagship immutable IRM that autonomously adapts to market conditions",
      "proxy": false,
      "keyEvents": [
        {
          "name": "BorrowRateUpdate",
          "description": "Emitted when borrow rate is updated"
        }
      ],
      "keyFunctions": [
        {
          "name": "borrowRateView",
          "mutability": "view"
        },
        {
          "name": "borrowRate",
          "mutability": "nonpayable"
        }
      ],
      "useCases": [
        "Track interest rate changes across markets over time",
        "Monitor rateAtTarget shifts and adaptation speed",
        "Build APY calculation interfaces"
      ],
      "github": "https://github.com/morpho-org/morpho-blue-irm"
    },
    "bundler": {
      "name": "Bundler (Bundler3)",
      "type": "helper",
      "description": "Call dispatcher enabling atomic execution of multiple protocol interactions in a single transaction",
      "proxy": false,
      "keyEvents": [
        {
          "name": "Multicall",
          "description": "Emitted when multiple calls are batched"
        }
      ],
      "useCases": [
        "Track bundled transaction patterns and sequences",
        "Monitor adapter usage distribution",
        "Measure gas savings vs unbundled transactions"
      ],
      "github": "https://github.com/morpho-org/bundler3"
    }
  },
